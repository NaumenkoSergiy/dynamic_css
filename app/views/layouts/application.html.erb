<!DOCTYPE html>
<html>
<head>
  <title>DynamicCss</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <% if current_user %>
  	<link href="<%= stylesheet_path(current_user.stylesheet, :format => :css) %>" rel="stylesheet" type="text/css" />
  <% end %>
</head>
<body>

<%= yield %>

</body>
</html>
